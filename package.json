{
  "name": "gh-profile",
  "version": "0.4.0",
  "description": "CLI tool to generate GitHub profile READMEs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gh-profile": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest"
  },
  "keywords": [
    "github",
    "profile",
    "readme",
    "cli",
    "generator"
  ],
  "author": "Abdulquddus ibrahim",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/chalk": "^2.2.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.0",
    "prettier": "^3.5.0",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^13.1.0",
    "lodash-es": "^4.17.23",
    "octokit": "^4.1.4",
    "ora": "^9.0.0"
  }
}