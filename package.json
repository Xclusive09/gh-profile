{
  "name": "gh-profile",
  "version": "1.0.0",
  "description": "CLI tool to generate GitHub profile READMEs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gh-profile": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest"
  },
  "keywords": [
    "github",
    "profile",
    "readme",
    "cli",
    "generator"
  ],
  "author": "Abdulquddus ibrahim",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.0",
    "prettier": "^3.5.0",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.0"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "octokit": "^4.1.4"
  }
}
